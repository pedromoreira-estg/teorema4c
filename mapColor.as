package  {		import flash.display.MovieClip;		// aplicação	public class mapColor extends MovieClip {				// mapas disponíveis na biblioteca		map1 : map;						// mapa activo		var themap : map;		public function mapColor() {			themap = new map1();			// posicionar ao centro			themap.x = stage.stageWidth/2;			themap.y = stage.stageHeight/2;			// escalar para dimensões conhecidas (supondo quadrado)			themap.width = 300;			themap.height = 300;			// adicionar ao stage (tornar visivel)			addChild(themap);		}			}	}