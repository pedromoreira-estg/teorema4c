package  {		import flash.display.MovieClip;	import flash.geom.ColorTransform;	import flash.events.MouseEvent;		public class regiao extends MovieClip {				var adjacencias : Array = new Array();		public var cor : uint = regiao.BRANCO;				public static const BRANCO		= 0;		public static const VERMELHO 	= 1;		public static const VERDE 		= 2;		public static const AZUL 			= 3;		public static const AMARELO	 	= 4;				public static const NCORES		= 5;						public function regiao() {			// constructor code			addEventListener(MouseEvent.CLICK,mudaCor);		}				public function mudaCor(e:MouseEvent) {			var mycor = this.cor;			mycor++;			this.setCor(mycor % regiao.NCORES);		}				public function setAdjacencias(a) {			adjacencias=a;		}				public function mostraAdjacencias() {			for each (var adj in adjacencias) {				trace(adj);			}		}				public function setCor(cor) {			var ct : ColorTransform;			switch (cor) {				case BRANCO :					 ct = new ColorTransform(1,1,1);					break;				case VERMELHO : 					 ct = new ColorTransform(1,0,0);					break;				case VERDE :					 ct = new ColorTransform(0,1,0);					break;				case AZUL :					 ct = new ColorTransform(0,0,1);					break;				case AMARELO :					 ct = new ColorTransform(1,1,0);					break;			}			transform.colorTransform = ct;			this.cor = cor;			var conflito : Boolean = false;						if (cor != regiao.BRANCO) {				//trace("-cor",cor);				for(var i=0; i<adjacencias.length; i++) {					if (adjacencias[i].cor == cor) {												conflito = true;						break;					}				}							trace(conflito);			}		}			}	}