package  {		import flash.display.MovieClip;			public class showstatus extends MovieClip {				public static const INCOMPLETO : String = "incompleto";		public static const INCORRETO	 : String = "incorreto";		public static const RESOLVIDO	 : String = "resolvido";		public function showstatus() {			reset();		}				public function reset () {			estado = INCOMPLETO;		}		public function set estado(status) {			if (status == INCOMPLETO) {				gotoAndStop(INCOMPLETO);			} else if (status == INCORRETO) {				gotoAndStop(INCORRETO);			} else if (status == RESOLVIDO) {				gotoAndStop(RESOLVIDO);			}		}	}	}